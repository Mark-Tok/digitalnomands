{
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "no-unused-vars": "warn",
      "no-extra-boolean-cast":"off",
      "no-console": "warn",
      "no-unused-expressions": "error",
      "import/order": [
        "error",
        {
          "newlines-between": "always-and-inside-groups",
          "pathGroups": [
            {
              "pattern": "react",
              "group": "builtin",
              "position": "before"
            },
            {
              "pattern": "~/**",
              "group": "external",
              "position": "after"
            }
          ],
          "pathGroupsExcludedImportTypes": ["react"]
        }
      ]
    }
  }